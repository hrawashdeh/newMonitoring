# Development Environment Variables
# These settings are used during local development

# Frontend Logging Configuration
VITE_ENABLE_CONSOLE_LOGS=true
VITE_ENABLE_BACKEND_LOGS=false
VITE_MIN_CONSOLE_LEVEL=DEBUG
VITE_MIN_BACKEND_LEVEL=WARN
VITE_LOG_BATCH_SIZE=10
VITE_LOG_FLUSH_INTERVAL=5000

# API Configuration
VITE_API_BASE_URL=http://localhost:30080
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
