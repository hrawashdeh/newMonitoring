# Production Environment Variables
# These settings are used when building for production

# Frontend Logging Configuration
VITE_ENABLE_CONSOLE_LOGS=false
VITE_ENABLE_BACKEND_LOGS=true
VITE_MIN_CONSOLE_LEVEL=ERROR
VITE_MIN_BACKEND_LEVEL=WARN
VITE_LOG_BATCH_SIZE=20
VITE_LOG_FLUSH_INTERVAL=10000

# API Configuration
# Use relative path - nginx proxies /api/* to gateway
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
